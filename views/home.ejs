
    <%- include("partials/header") %> 

    <h1>Home</h1>
    <p><%= homeStartingContent %> </p>
    <% posts.forEach(function(element) { %>
        <h1><%= element.title %> </h1>
        <!-- <p><%= element.content %></p>  -->
        <% if (element.content.length>120) { %>
            <% var s=element.content.substring(0,120)+"..." %> 
         <p><%= s %> <a href="/posts/<%= element.title %> ">Read More</a></p>
        <% } else { %>
            <%= element.content %>  
            <% } %> 
    <% }); %> 
        

    <%- include('partials/footer') %> 

    
